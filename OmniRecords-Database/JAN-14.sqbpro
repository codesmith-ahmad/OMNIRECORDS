drop trigger if exists documents_filepath;

CREATE TRIGGER documents_filepath_insert
AFTER INSERT ON &quot;documents&quot;
BEGIN
	UPDATE &quot;documents&quot;
	SET file = folderpath || &quot;\&quot; || replace(lower(trim(document)),&quot; &quot;,&quot;_&quot;) || &quot;.&quot; || type
	from (select value as folderpath from metadata where key = &quot;documents_folderpath&quot;);
END;

CREATE TRIGGER documents_filepath_update
AFTER UPDATE ON &quot;documents&quot;
BEGIN
	UPDATE &quot;documents&quot;
	SET file = folderpath || &quot;\&quot; || replace(lower(trim(document)),&quot; &quot;,&quot;_&quot;) || &quot;.&quot; || type
	from (select value as folderpath from metadata where key = &quot;documents_folderpath&quot;);
END;</sql><current_tab id="5"/></tab_sql></sqlb_project>
